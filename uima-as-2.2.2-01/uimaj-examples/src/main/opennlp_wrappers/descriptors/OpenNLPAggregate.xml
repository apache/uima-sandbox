<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>false</primitive>
  <delegateAnalysisEngineSpecifiers>
    <delegateAnalysisEngine key="OpenNLPSentenceDetector">
      <import location="OpenNLPSentenceDetector.xml"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="OpenNLPPOSTagger">
      <import location="OpenNLPPOSTagger.xml"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="OpenNLPTokenizer">
      <import location="OpenNLPTokenizer.xml"/>
    </delegateAnalysisEngine>
  </delegateAnalysisEngineSpecifiers>
  <analysisEngineMetaData>
    <name>OpenNLPAggregate</name>
    <description>Aggregate analysis engine that performs sentence detection, tokenization, and POS tagging using OpenNLP.</description>
    <version>1.0</version>
    <vendor>The Apache Software Foundation</vendor>
    <configurationParameters searchStrategy="language_fallback"/>
    <configurationParameterSettings/>
    <flowConstraints>
      <fixedFlow>
        <node>OpenNLPSentenceDetector</node>
        <node>OpenNLPTokenizer</node>
        <node>OpenNLPPOSTagger</node>
      </fixedFlow>
    </flowConstraints>
    <capabilities>
      <capability>
        <inputs/>
        <outputs>
          <type>org.apache.uima.examples.opennlp.Token</type>
          <type>org.apache.uima.examples.opennlp.Sentence</type>
          <type allAnnotatorFeatures="true">org.apache.uima.examples.opennlp.X</type>
          <type allAnnotatorFeatures="true">org.apache.uima.examples.opennlp.WHPP</type>
          <type allAnnotatorFeatures="true">org.apache.uima.examples.opennlp.WHNP</type>
          <type allAnnotatorFeatures="true">org.apache.uima.examples.opennlp.WHAVP</type>
          <type allAnnotatorFeatures="true">org.apache.uima.examples.opennlp.WHADJP</type>
          <type allAnnotatorFeatures="true">org.apache.uima.examples.opennlp.VP</type>
          <type allAnnotatorFeatures="true">org.apache.uima.examples.opennlp.UCP</type>
          <type allAnnotatorFeatures="true">org.apache.uima.examples.opennlp.Time</type>
          <type allAnnotatorFeatures="true">org.apache.uima.examples.opennlp.SyntaxAnnotation</type>
          <type allAnnotatorFeatures="true">org.apache.uima.examples.opennlp.SQ</type>
          <type allAnnotatorFeatures="true">org.apache.uima.examples.opennlp.SINV</type>
          <type allAnnotatorFeatures="true">org.apache.uima.examples.opennlp.SBARQ</type>
          <type allAnnotatorFeatures="true">org.apache.uima.examples.opennlp.SBAR</type>
          <type allAnnotatorFeatures="true">org.apache.uima.examples.opennlp.S</type>
          <type allAnnotatorFeatures="true">org.apache.uima.examples.opennlp.RRC</type>
          <type allAnnotatorFeatures="true">org.apache.uima.examples.opennlp.QP</type>
          <type allAnnotatorFeatures="true">org.apache.uima.examples.opennlp.Phrase</type>
          <type allAnnotatorFeatures="true">org.apache.uima.examples.opennlp.Person</type>
          <type allAnnotatorFeatures="true">org.apache.uima.examples.opennlp.Percentage</type>
          <type allAnnotatorFeatures="true">org.apache.uima.examples.opennlp.PRT</type>
          <type allAnnotatorFeatures="true">org.apache.uima.examples.opennlp.PRNphrase</type>
          <type allAnnotatorFeatures="true">org.apache.uima.examples.opennlp.PP</type>
          <type allAnnotatorFeatures="true">org.apache.uima.examples.opennlp.Organization</type>
          <type allAnnotatorFeatures="true">org.apache.uima.examples.opennlp.NX</type>
          <type allAnnotatorFeatures="true">org.apache.uima.examples.opennlp.NP</type>
          <type allAnnotatorFeatures="true">org.apache.uima.examples.opennlp.NAC</type>
          <type allAnnotatorFeatures="true">org.apache.uima.examples.opennlp.Money</type>
          <type allAnnotatorFeatures="true">org.apache.uima.examples.opennlp.Location</type>
          <type allAnnotatorFeatures="true">org.apache.uima.examples.opennlp.LST</type>
          <type allAnnotatorFeatures="true">org.apache.uima.examples.opennlp.INTJ</type>
          <type allAnnotatorFeatures="true">org.apache.uima.examples.opennlp.FRAG</type>
          <type allAnnotatorFeatures="true">org.apache.uima.examples.opennlp.EntityAnnotation</type>
          <type allAnnotatorFeatures="true">uima.tcas.DocumentAnnotation</type>
          <type allAnnotatorFeatures="true">org.apache.uima.examples.opennlp.Date</type>
          <type allAnnotatorFeatures="true">org.apache.uima.examples.opennlp.Clause</type>
          <type allAnnotatorFeatures="true">org.apache.uima.examples.opennlp.CONJP</type>
          <type allAnnotatorFeatures="true">uima.tcas.Annotation</type>
          <type allAnnotatorFeatures="true">org.apache.uima.examples.opennlp.ADVP</type>
          <type allAnnotatorFeatures="true">org.apache.uima.examples.opennlp.ADJP</type>
        </outputs>
        <languagesSupported/>
      </capability>
    </capabilities>
    <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
    </operationalProperties>
  </analysisEngineMetaData>
  <resourceManagerConfiguration/>
</analysisEngineDescription>
