  <version>1.0</version>
  <vendor>TEMIS</vendor>
  <types>
    <typeDescription>
      <name>org.apache.uima.annotator.bsf.types.Token</name>
      <description></description>
      <supertypeName>uima.tcas.Annotation</supertypeName>
