    </typeDescription>
    <typeDescription>
      <name>org.apache.uima.annotator.bsf.types.Firstname</name>
      <description></description>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features/>
    </typeDescription>
    <typeDescription>
      <name>org.apache.uima.annotator.bsf.types.Nickname</name>
      <description></description>
      <supertypeName>org.apache.uima.annotator.bsf.types.Firstname</supertypeName>
      <features>
        <featureDescription>
        <name>firstnames</name>
        <description>List of possible firstnames related to this nickname</description>
        <rangeTypeName>uima.cas.StringArray</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
  </types>
</typeSystemDescription>
