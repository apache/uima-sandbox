        <inputQueue endpoint="MeetingDetectorTaeQueue"
          brokerURL="tcp://tcp.main.input.queue.localhost:61616"/>
        <topDescriptor>         
          <import location="cppDescr.xml"/>
        </topDescriptor>
        
        <environmentVariables>
          <environmentVariable name="e1">some text</environmentVariable>
          <environmentVariable name="e2"></environmentVariable>
        </environmentVariables>
        
      </service>

  </deployment>
  
</analysisEngineDeploymentDescription>