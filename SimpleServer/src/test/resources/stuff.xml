<?xml version="1.0" encoding="UTF-8"?>
<uimaSimpleServerSpec xmlns="http://incubator.apache.org/uima/simpleserver/config/xml">
	<shortDescription>Recognize stuff</shortDescription>
	<type name="org.apache.uima.Stuff" outputTag="Stuff" outputCoveredText="on">
		<filters>
			<filter featurePath="feat" condition="not null"/>
		</filters>
		<outputs>
			<output featurePath="ashes" outputAttribute="ashes"/>
			<output featurePath="dust" outputAttribute="dust"/>
		</outputs>
	</type>
	
</uimaSimpleServerSpec>
