<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
<extension-point id="tmTestEngine" name="tmTestEngine" schema="schema/tmTestEngine.exsd"/>
   <extension
         point="org.eclipse.debug.core.launchConfigurationTypes">
      <launchConfigurationType
            delegate="org.apache.uima.tm.dltk.internal.testing.TextMarkerTestingLaunchConfigurationDelegate"
            id="org.apache.uima.tm.dltk.testing.launchConfig"
            modes="run, debug"
            name="TextMarker Testing"
            public="true"
            sourceLocatorId="tmSourceLocator"
            sourcePathComputerId="tmSourcePathComputer">
      </launchConfigurationType>
   </extension>
   <extension
         point="org.eclipse.debug.ui.launchConfigurationTypeImages">
      <launchConfigurationTypeImage
            configTypeID="org.apache.uima.tm.dltk.testing.launchConfig"
            icon="icons/full/obj16/textmarker.png"
            id="org.de.uniwue.dltk.textmarker.testing.launchConfigImage">
      </launchConfigurationTypeImage>
   </extension>
   <extension
         point="org.eclipse.debug.ui.launchConfigurationTabGroups">
      <launchConfigurationTabGroup
            class="org.apache.uima.tm.dltk.internal.testing.TextMarkerTestingTabGroup"
            id="org.apache.uima.tm.dltk.testing.launchConfigTabGroup"
            type="org.apache.uima.tm.dltk.testing.launchConfig">
         <launchMode
               description="Run mode"
               mode="run">
         </launchMode>
         <launchMode
               mode="debug"
               perspective="org.eclipse.debug.ui.DebugPerspective">
         </launchMode>
      </launchConfigurationTabGroup>
   </extension>
